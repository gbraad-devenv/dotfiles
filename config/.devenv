[devenv]
	systemd = true
	aliases = true
	workdir = /var/home/gbraad
	shell = /bin/zsh

[images]
	dotcentos = ghcr.io/gbraad-dotfiles/centos:latest
	dotfedora = ghcr.io/gbraad-dotfiles/fedora:latest
	dotdebian = ghcr.io/gbraad-dotfiles/debian:latest
	fed = ghcr.io/gbraad-devenv/fedora/systemd:41
	code = ghcr.io/gbraad-devenv/fedora/code-cli:41
	desk = ghcr.io/gbraad-devenv/fedora/rdesktop:41
	go = ghcr.io/gbraad-devenv/fedora-golang/systemd:41
	fed40 = ghcr.io/gbraad-devenv/fedora/systemd:40
	deb = ghcr.io/gbraad-devenv/debian/systemd:bookworm
	alp = ghcr.io/gbraad-devenv/alpine/systemd:3.18
	cen = ghcr.io/gbraad-devenv/centos/systemd:stream10
	gobi = ghcr.io/gbraad-devenv/ubi9-gotoolset/systemd:1.22.7
	ubi = ghcr.io/gbraad-devenv/ubi/systemd:9
	ubu = ghcr.io/gbraad-devenv/ubuntu/systemd:jammy
	alm = ghcr.io/gbraad-devenv/almalinux/systemd:9
	sus = ghcr.io/gbraad-devenv/opensuse/systemd:15.5
	tum = ghcr.io/gbraad-devenv/tumbleweed/systemd:latest
	tail = ghcr.io/spotsnel/tailscale-systemd/fedora:latest
	obsidian = ghcr.io/gbraad-apps/obsidian:latest
	nextcloud = ghcr.io/gbraad-homelab/nextcloud/hub9:latest
	forgejo = ghcr.io/gbraad-homelab/private-forgejo:latest
	jellyfin = ghcr.io/gbraad-homelab/jellyfin:latest
	cardforge  = ghcr.io/gbraad-gaming/cardforge:latest
	steam = ghcr.io/gbraad-gaming/steam:latest
	steamcmd = ghcr.io/gbraad-gaming/steamcmd:latest

